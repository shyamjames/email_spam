        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="231dp" />

    <EditText
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="307dp"
        android:layout_height="38dp"
        android:ems="10"
